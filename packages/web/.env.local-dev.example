# Local Development Environment Variables Example
# Copy this file to .env.local-dev and configure for your local development

# Current environment identifier
VITE_CURRENT_ENV=local-dev

# API Configuration
VITE_API_BASE_URL=http://localhost:5173

# Firebase Configuration - Uses emulated services
# These values connect to Firebase emulators running locally
VITE_FIREBASE_EMULATOR_REQUIRED=true
VITE_FIREBASE_API_KEY=demo-api-key
VITE_FIREBASE_AUTH_DOMAIN=demo-local-dev.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=demo-local-dev
VITE_FIREBASE_STORAGE_BUCKET=demo-local-dev.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=3456789012
VITE_FIREBASE_APP_ID=3:123456789:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-3456789012

# Development flags
VITE_DEV_MODE=true
